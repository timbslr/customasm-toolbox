{
	"name": "customasm-toolbox",
	"displayName": "customasm-toolbox",
	"description": "",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.105.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "customasm-toolbox.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "customasm-assembly",
				"aliases": [
					"customasm-assembly",
					"customasm-assembly"
				],
				"extensions": [
					".asm"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "customasm-assembly",
				"scopeName": "source.customasm-assembly",
				"path": "./src/syntaxes/customasm-assembly.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.19.0",
		"@types/vscode": "^1.105.0",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.36.0"
	}
}
